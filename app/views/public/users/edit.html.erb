<%= form_with model: @user, url:user_path, method: :patch, local:true do |f| %>
  <%= f.label :"名前" %>
  <%= f.text_field :name %>
  
  <%= f.label :"画像" %>
  <%= f.file_field :image %>
  
  <%= f.label :"プローフィール" %>
  <%= f.text_area :introduction %>
  
  <%= f.label :"性別" %>
  <%= f.text_field :sex %>
  
  <%= f.label :"年齢" %>
  <%= f.text_field :age %>
  
  
<% end %>